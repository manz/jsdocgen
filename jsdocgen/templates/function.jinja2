{% import 'macroes.html' as utils %}

{% if function.kind == 'typedef' %}
{{ utils.render_title(function, 'function definition') }}
{% else %}
{{ utils.render_title(function, 'function') }}
{% endif %}

<code>{{ function['longname'] }}</code><br/><br/>
<div markdown="1">

{% if function.examples %}
{{ '{% raw %}' }}
{% for example in function.examples %}
### {{ example }}

{% endfor %}
{{ '{% endraw %}' }}
{% endif %}

<table class="function">
    <thead>
    <tr>
        <td colspan="2">Function</td>
    </tr>
    </thead>
    <tbody>
    {% with method=function %}
    {% include 'method.jinja2' %}
    {% endwith %}
    </tbody>
</table>
</div>
